{
  "manifest_version": 2,

  "name": "Poker Oddy",
  "description": "This extension calcules pot odds, for online poker games",
  "version": "1.0",

  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}